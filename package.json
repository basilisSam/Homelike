{
  "name": "homelike",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.11.4",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.105",
    "@types/react": "^17.0.80",
    "@types/react-dom": "^17.0.25",
    "graphql": "^16.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
